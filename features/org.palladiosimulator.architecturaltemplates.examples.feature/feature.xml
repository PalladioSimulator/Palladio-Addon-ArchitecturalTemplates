<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.architecturaltemplates.examples.feature"
      label="Architectural Templates - Default Catalog"
      version="6.0.0.qualifier"
      provider-name="palladiosimulator.org"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="http://wiki.cloudscale-project.eu/index.php/HowTos">
      Default AT catalog provides best practice ATs for distributed computing (layered architectures, loadbalancers, caches), cloud computing (horizontal and vertical scaling), and big data (Hadoop MapReduce).
   </description>

   <requires>
      <import plugin="org.eclipse.core.runtime" version="3.10.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.m2m.qvt.oml" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.m2m.qvt.oml.project" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.modelversioning.emfprofile" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="org.modelversioning.emfprofile.registry" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf" version="2.6.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.identifier" version="2.1.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.pcm" version="4.0.1" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.analyzer.completions" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.mdsdprofiles" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.mdsdprofiles.api" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.simucomframework" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.simucomframework.variables" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.architecturaltemplates.ui" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.branding"/>
      <!-- cost is required by some initiator templates -->
      <import feature="org.palladiosimulator.cost.feature" version="1.0.0.qualifier"/>
   </requires>

   <plugin
         id="org.palladiosimulator.architecturaltemplates.catalog"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
