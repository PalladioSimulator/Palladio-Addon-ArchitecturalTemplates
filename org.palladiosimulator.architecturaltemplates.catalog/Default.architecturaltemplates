      <constraints xsi:type="org.palladiosimulator.architecturaltemplates:OCLConstraint" id="_pUzH0YxrEeWR64PY5KUWug" entityName="Presentation Layer Assemblies do not require functionality from Data Layer Assemblies" expression="self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::AssemblyConnector)->forAll(ac|not(ac.requiringAssemblyContext_AssemblyConnector.hasAppliedStereotype('PresentationLayerAssembly') and ac.providingAssemblyContext_AssemblyConnector.hasAppliedStereotype('DataLayerAssembly'))) and&#xD;&#xA;self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::AssemblyInfrastructureConnector)->forAll(ac|not(ac.requiringAssemblyContext__AssemblyInfrastructureConnector.hasAppliedStereotype('PresentationLayerAssembly') and ac.providingAssemblyContext__AssemblyInfrastructureConnector.hasAppliedStereotype('DataLayerAssembly')))"/>
      <constraints xsi:type="org.palladiosimulator.architecturaltemplates:OCLConstraint" id="_v8NgsYxrEeWR64PY5KUWug" entityName="Data Layer Assemblies do not require functionality from Middle Layer Assemblies" expression="self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::AssemblyConnector)->forAll(ac|not(ac.requiringAssemblyContext_AssemblyConnector.hasAppliedStereotype('DataLayerAssembly') and ac.providingAssemblyContext_AssemblyConnector.hasAppliedStereotype('MiddleLayerAssembly'))) and&#xD;&#xA;self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::AssemblyInfrastructureConnector)->forAll(ac|not(ac.requiringAssemblyContext__AssemblyInfrastructureConnector.hasAppliedStereotype('DataLayerAssembly') and ac.providingAssemblyContext__AssemblyInfrastructureConnector.hasAppliedStereotype('MiddleLayerAssembly')))"/>
      <constraints xsi:type="org.palladiosimulator.architecturaltemplates:OCLConstraint" id="_2MJ0wYxrEeWR64PY5KUWug" entityName="No Provided Delegation Connectors to Middle Layer Assemblies" expression="self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::ProvidedDelegationConnector)->forAll(pdc|not(pdc.assemblyContext_ProvidedDelegationConnector.hasAppliedStereotype('MiddleLayerAssembly')))"/>
      <constraints xsi:type="org.palladiosimulator.architecturaltemplates:OCLConstraint" id="_I8FI4c6qEeWtjIp7SFTxwg" entityName="No Provided Infrastructure Delegation Connectors to Middle Layer Assemblies" expression="self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::ProvidedInfrastructureDelegationConnector)->forAll(pdc|not(pdc.assemblyContext__ProvidedInfrastructureDelegationConnector.hasAppliedStereotype('MiddleLayerAssembly')))"/>
      <constraints xsi:type="org.palladiosimulator.architecturaltemplates:OCLConstraint" id="_81ECcYxrEeWR64PY5KUWug" entityName="No Provided Delegation Connectors to Data Layer Assemblies" expression="self.appliedTo.oclAsType(pcm::system::System).connectors__ComposedStructure->selectByType(pcm::core::composition::ProvidedDelegationConnector)->forAll(pdc|not( pdc.assemblyContext_ProvidedDelegationConnector.hasAppliedStereotype('DataLayerAssembly')))"/>
